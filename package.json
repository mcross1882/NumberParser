{
  "name": "numberparser",
  "version": "0.0.4",
  "description": "A simple number formatting utility for Javascript and Node.js",
  "main": "numberparser.js",
  "scripts": {
    "test": "mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mcross1882/NumberParser"
  },
  "keywords": [
    "number",
    "formatting",
    "printf",
    "zerofill",
    "rounding"
  ],
  "author": "Matthew Cross <blacklightgfx@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcross1882/NumberParser/issues"
  },
  "devDependencies": {
    "blanket": "1.1.7",
    "mocha": "2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "coveralls": "2.11.3"
  },
  "config": {
    "blanket": {
      "pattern": [
        "numberparser.js"
      ],
      "data-cover-never": ["node_modules"]
    }
  }
}
